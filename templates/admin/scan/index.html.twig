{% extends 'admin_base.html.twig' %}


{% block title %}Scan index{% endblock %}

{% block body %}
    <h1>Scan index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Title</th>
                <th>Image</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for scan in scans %}
            <tr>
                <td>{{ scan.id }}</td>
                <td>{{ scan.title }}</td>
                <td>{{ scan.image }}</td>
                <td>
                    <a href="{{ path('scan_show', {'id': scan.id}) }}">show</a>
                    <a href="{{ path('scan_edit', {'id': scan.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('scan_new') }}">Create new</a>
{% endblock %}
